<section>
  <div class="your-friends">
    <h3 class="mt-3">Your Friends - <span id="friend-count" class="text-primary">{{friendCount}}</span></h3>
    {{#each friends}}
      <div class="friend-card">
        <a href="/{{this._id}}">
        <img
          src="profile-images/{{this._id}}.jpg"
          alt="img"
          style="width:5rem"
        />
        </a>
        <a href="/{{this._id}}"><h5 class="m-4">{{this.Name}}</h5></a>
      </div>
    {{/each}}
  </div>
</section>